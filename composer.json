{
  "name": "desrodman/yii2-localeurls",
  "description": "Automatic locale/language management for URLs.",
  "keywords": ["yii2", "i18n", "locale"],
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Dennis Galan",
      "email": "dennis@prof-email.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "*"
  },
  "require-dev": {
    "phpunit/phpunit": ">4.0 <8"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "desrodman\\localeurls\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": "tests"
    }
  }
}
